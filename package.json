{
  "name": "vue-tinybox",
  "version": "1.3.0",
  "description": "A slick, yet tiny lightbox gallery for Vue.js",
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs",
    "vue.js",
    "lightbox",
    "gallery"
  ],
  "homepage": "https://os.karamoff.dev/vue-tinybox",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NickKaramoff/vue-tinybox.git"
  },
  "bugs": {
    "url": "https://github.com/NickKaramoff/vue-tinybox/issues"
  },
  "author": {
    "name": "Nikita Karamov",
    "email": "nick@karamoff.dev",
    "url": "https://karamoff.dev"
  },
  "main": "./dist/tinybox.umd.js",
  "module": "./dist/tinybox.esm.js",
  "unpkg": "./dist/tinybox.umd.js",
  "exports": {
    "require": "./dist/tinybox.umd.js",
    "import": "./dist/tinybox.esm.js"
  },
  "browser": {
    "./sfc": "src/tinybox.vue"
  },
  "files": [
    "dist/tinybox.umd.js",
    "dist/tinybox.esm.js",
    "src/tinybox.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev:build": "rollup -c -w --no-watch.clearScreen",
    "dev:serve": "sirv ./dev -s -D -p 8629",
    "dev": "run-p dev:*",
    "test:lint": "eslint .",
    "test:size": "size-limit",
    "pretest": "run-s build",
    "test": "run-p --aggregate-output test:*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@size-limit/preset-small-lib": "^4.12.0",
    "autoprefixer": "^9.8.8",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.39",
    "rollup": "^2.79.1",
    "rollup-plugin-vue": "^5.1.9",
    "sirv-cli": "^1.0.14",
    "size-limit": "^4.12.0",
    "vue-eslint-parser": "^7.11.0",
    "vue-template-compiler": "^2.7.14"
  },
  "size-limit": [
    {
      "path": "dist/tinybox.umd.js",
      "limit": "3333 B",
      "webpack": false
    },
    {
      "path": "dist/tinybox.esm.js",
      "limit": "3333 B"
    }
  ],
  "engines": {
    "node": ">=10"
  }
}
