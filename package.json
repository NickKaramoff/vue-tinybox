{
  "name": "vue-tinybox",
  "version": "0.2.0",
  "description": "A slick, yet tiny lightbox gallery component for Vue.js",
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs",
    "vue.js",
    "lightbox",
    "gallery"
  ],
  "homepage": "https://github.com/NickKaramoff/vue-tinybox#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NickKaramoff/vue-tinybox.git"
  },
  "bugs": {
    "url": "https://github.com/NickKaramoff/vue-tinybox/issues"
  },
  "author": {
    "name": "Nikita Karamov",
    "email": "nick@karamoff.ru",
    "url": "https://karamoff.ru"
  },
  "main": "dist/tinybox.umd.js",
  "module": "dist/tinybox.esm.js",
  "files": [
    "dist/tinybox.umd.js",
    "dist/tinybox.esm.js",
    "src/Tinybox.vue"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.prod.js",
    "dev": "rollup -w --config rollup.config.dev.js",
    "lint": "eslint ./src",
    "size": "size-limit",
    "test": "npm run lint && npm run build && npm run size"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.1.1",
    "eslint": "^6.2.1",
    "eslint-plugin-vue": "^5.2.3",
    "rollup": "^1.20.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue-eslint-parser": "^6.0.4",
    "vue-template-compiler": "^2.6.10"
  },
  "size-limit": [
    {
      "path": "dist/tinybox.umd.js",
      "limit": "4 KB"
    }
  ],
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ]
}
