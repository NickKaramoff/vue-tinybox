{
  "name": "vue-tinybox",
  "version": "0.3.0",
  "description": "A slick, yet tiny lightbox gallery component for Vue.js",
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs",
    "vue.js",
    "lightbox",
    "gallery"
  ],
  "homepage": "https://os.karamoff.dev/vue-tinybox",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NickKaramoff/vue-tinybox.git"
  },
  "bugs": {
    "url": "https://github.com/NickKaramoff/vue-tinybox/issues"
  },
  "author": {
    "name": "Nikita Karamov",
    "email": "nick@karamoff.dev",
    "url": "https://karamoff.dev"
  },
  "main": "dist/tinybox.umd.js",
  "module": "dist/tinybox.esm.js",
  "files": [
    "dist/tinybox.umd.js",
    "dist/tinybox.esm.js",
    "src/Tinybox.vue"
  ],
  "scripts": {
    "build": "rollup --config ./build/rollup.config.prod.js",
    "dev": "rollup -w --config ./build/rollup.config.dev.js",
    "test:lint": "eslint .",
    "test:size": "size-limit",
    "checks": "run-s test:lint build test:size"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@size-limit/preset-small-lib": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-vue": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.4",
    "vue-eslint-parser": "^7.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "size-limit": [
    {
      "path": "dist/tinybox.umd.js",
      "limit": "3.5 KB"
    }
  ],
  "browserslist": [
    ">=.5%, last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=8"
  }
}
